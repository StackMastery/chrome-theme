{
  "manifest_version": 3,
  "name": "Dev Theme",
  "version": "1.0",
  "description": "A custom HTML page for the new tab.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },

  "permissions": [
    "storage",   
    "tabs"          
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
